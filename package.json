{
  "name": "tailwindcss-aria-attributes",
  "version": "1.3.0",
  "description": "aria-* attribute utility plugin for TailwindCSS",
  "keywords": [
    "tailwindcss",
    "tailwindcss-plugin",
    "aria",
    "aria-attributes",
    "aria-current"
  ],
  "homepage": "https://github.com/danestves/tailwindcss-aria-attributes",
  "repository": {
    "type": "git",
    "url": "git@github.com:danestves/tailwindcss-aria-attributes.git"
  },
  "license": "MIT",
  "author": "Daniel Esteves <danestves@gmail.com>",
  "main": "dist/index.js",
  "module": "dist/tailwindcss-aria-attributes.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "dts build",
    "changeset": "changeset add",
    "lint": "dts lint src",
    "prepare": "dts build",
    "release": "npm run build && changeset publish",
    "size": "size-limit",
    "start": "dts watch",
    "test": "dts test --passWithNoTests"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@changesets/changelog-github": "0.4.8",
    "@changesets/cli": "2.26.2",
    "@navith/tailwindcss-plugin-author-types": "1.8.1-0.1",
    "@rollup/plugin-replace": "5.0.5",
    "@size-limit/preset-small-lib": "10.0.2",
    "@typescript-eslint/eslint-plugin": "6.9.1",
    "@typescript-eslint/parser": "6.9.1",
    "dts-cli": "2.0.3",
    "eslint": "8.52.0",
    "eslint-config-prettier": "9.0.0",
    "husky": "8.0.3",
    "prettier": "3.0.3",
    "size-limit": "10.0.2",
    "tailwindcss": "3.3.5",
    "tslib": "2.6.2",
    "typescript": "5.2.2"
  },
  "peerDependencies": {
    "tailwindcss": ">=2.2"
  },
  "engines": {
    "node": ">=18"
  },
  "size-limit": [
    {
      "path": "dist/tailwindcss-aria-attributes.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/tailwindcss-aria-attributes.esm.js",
      "limit": "10 KB"
    }
  ]
}
