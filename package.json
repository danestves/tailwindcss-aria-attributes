{
  "name": "tailwindcss-aria-attributes",
  "version": "1.3.0",
  "description": "aria-* attribute utility plugin for TailwindCSS",
  "keywords": [
    "tailwindcss",
    "tailwindcss-plugin",
    "aria",
    "aria-attributes",
    "aria-current"
  ],
  "homepage": "https://github.com/danestves/tailwindcss-aria-attributes",
  "repository": {
    "type": "git",
    "url": "git@github.com:danestves/tailwindcss-aria-attributes.git"
  },
  "license": "MIT",
  "author": "Daniel Esteves <danestves@gmail.com>",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "changeset": "changeset add",
    "lint": "eslint .",
    "prepare": "tsup",
    "release": "npm run build && changeset publish",
    "size": "size-limit",
    "start": "tsup --watch"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@changesets/changelog-github": "0.5.0",
    "@changesets/cli": "2.27.1",
    "@rollup/plugin-replace": "5.0.5",
    "@size-limit/preset-small-lib": "11.0.1",
    "@typescript-eslint/eslint-plugin": "6.14.0",
    "@typescript-eslint/parser": "6.14.0",
    "eslint": "8.55.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.0.1",
    "husky": "8.0.3",
    "prettier": "3.1.1",
    "size-limit": "11.0.1",
    "tailwindcss": "3.3.6",
    "tslib": "2.6.2",
    "tsup": "8.0.1",
    "typescript": "5.3.3"
  },
  "peerDependencies": {
    "tailwindcss": ">=2.2"
  },
  "engines": {
    "node": ">=18"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "2 KB"
    },
    {
      "path": "dist/index.mjs",
      "limit": "2 KB"
    }
  ]
}
