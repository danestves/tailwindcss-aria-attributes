{
  "version": "1.0.2",
  "description": "aria-* attribute utility plugin for TailwindCSS",
  "keywords": [
    "tailwindcss",
    "tailwindcss-plugin",
    "aria",
    "aria-attributes",
    "aria-current"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "homepage": "https://github.com/danestves/tailwindcss-aria-attributes",
  "repository": {
    "type": "git",
    "url": "git@github.com:danestves/tailwindcss-aria-attributes.git"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "changeset": "changeset add",
    "release": "yarn build && changeset publish"
  },
  "peerDependencies": {
    "tailwindcss": ">=2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "tailwindcss-aria-attributes",
  "author": "Daniel Esteves <danestves@gmail.com>",
  "module": "dist/tailwindcss-aria-attributes.esm.js",
  "size-limit": [
    {
      "path": "dist/tailwindcss-aria-attributes.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/tailwindcss-aria-attributes.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.1",
    "@changesets/cli": "^2.18.0",
    "@navith/tailwindcss-plugin-author-types": "^1.8.1-0.1",
    "@rollup/plugin-replace": "^3.0.0",
    "@size-limit/preset-small-lib": "^6.0.4",
    "@typescript-eslint/eslint-plugin": "5.3.x",
    "@typescript-eslint/parser": "5.3.x",
    "@zazen/eslint-config": "2.3.x",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard-with-typescript": "21.0.1",
    "husky": "^7.0.4",
    "prettier": "^2.4.1",
    "size-limit": "^6.0.4",
    "tailwindcss": "^2.2.19",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4"
  }
}
